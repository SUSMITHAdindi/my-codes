import java.util.*;
public class Main
{
  public static void main (String[]args)
  {
    Scanner sc = new Scanner (System.in);
    int n =13;
    int a[]=new int[n];
    a[1]=1;
    for(int i=2;i<n;i++)
    a[i]=i*a[i-1];
    System.out.println("enter a value");
    int x=sc.nextInt();
    System.out.println("factorial of given number is "+a[x]);
  }  
}
----------------------------------------------------------------------------------------------------------------------------
OUTPUT:
enter a value
5
factorial of given number is 120
---------------------------------------------------------------------------------------------------------------------------
OUTPUT:
enter a value
8
factorial of given number is 40320
