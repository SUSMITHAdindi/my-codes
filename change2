import java.util.*;
class change
{
public static void main(String args[])
{
int n,a,b,r=1,num=0,c=0;
Scanner sc=new Scanner(System.in);
n=sc.nextInt();
a=sc.nextInt();
b=sc.nextInt();
while(n>0)
{
 int rem=n%10;
if(rem==a)
num=num+r*b;
else
num=num+r*rem;
r=r*10;
n=n/10;
}
System.out.println(num);
}
}
------------------------------------------------------------------------------
output:
1234511
1
7
7234577
